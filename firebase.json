{
  "functions": {
    "predeploy": [
      "npm --prefix \"$RESOURCE_DIR\" run lint"
    ]
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
      "source": "**/*.@(js|css|fontawesome-webfont.woff2?v=4.7.0)",
      "headers": [{
        "key": "Cache-Control",
        "value": "max-age=15552000"
      }]
    }, {
      "source": "**/*.@(jpg|jpeg|gif|png|webp|svg|eot|otf|ttf|ttc|woff)",
      "headers": [{
        "key": "Cache-Control",
        "value": "max-age=15552000"
      }]
    }, {
      "source": "404.html",
      "headers": [{
        "key": "Cache-Control",
        "value": "max-age=300"
      }]
    }]
,
    "storage": {
      "rules": "storage.rules"
    },
    "cleanUrls": true
    
  }
}